# SMX LiveBoard Configuration
NODE_ENV=development
PORT=3000

# Monitoring Settings (Otimizado)
METRICS_INTERVAL=10000
PROCESSES_INTERVAL=60000
SERVICES_INTERVAL=30000

# Security
CORS_ORIGIN=*

# Logging
LOG_LEVEL=info
LOG_CLEANUP_DAYS=7

# System Limits
MAX_HISTORY_POINTS=60
MAX_PROCESSES=10
MAX_SERVICES=20

